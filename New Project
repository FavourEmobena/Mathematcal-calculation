# Addition, subtraction
print(5 + 5)
print(5 - 5)

# Multiplication, division, modulo, and exponentiation
print(3 * 5)
print(10 / 2)
print(18 % 7)
print(4 ** 2)

# How much is your $100 worth after 7 years?
# Addition
print(5+5)
#subtraction
print(5-5)
# multiplication
print(3*5)
# division(10/2)
# modulo
print(18%7)
# exponentiation
print(4**2)
